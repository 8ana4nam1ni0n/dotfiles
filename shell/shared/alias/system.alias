#!/bin/sh

# List Directory
alias ls='ls --color=auto'
alias ll='ls -AlF'
alias la='ls -A'
alias l.='ls -d .* 2>/dev/null'


# grep
alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'


# change directories
alias ..='cd ..'
alias cd..='cd ..'
alias ...='cd ../../'

